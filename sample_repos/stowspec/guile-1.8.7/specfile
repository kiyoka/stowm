;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/guile-1.8.7 
")
 (make . "make")
 (install . "make install")
 (arc . "guile-1.8.7.tar.gz")
 (url . "http://core.ring.gr.jp/pub/GNU/guile/guile-1.8.7.tar.gz")
 (projname . "guile-1.8.7")
))
