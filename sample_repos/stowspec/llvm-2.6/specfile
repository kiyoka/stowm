;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/llvm-2.6 
")
 (make . "make")
 (install . "make install")
 (arc . "llvm-2.6.tar.gz")
 (url . "http://llvm.org/releases/2.6/llvm-2.6.tar.gz")
 (projname . "llvm-2.6")
))
