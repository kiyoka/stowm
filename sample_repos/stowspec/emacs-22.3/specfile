;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/emacs-22.3 
")
 (make . "make")
 (install . "make install")
 (arc . "emacs-22.3.tar.gz")
 (url . "http://www.t.ring.gr.jp/archives/GNU/emacs/emacs-22.3.tar.gz")
 (projname . "emacs-22.3")
))
