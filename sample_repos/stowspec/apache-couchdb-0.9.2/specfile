;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/apache-couchdb-0.9.2 
")
 (make . "make")
 (install . "make install")
 (arc . "apache-couchdb-0.9.2.tar.gz")
 (url . "http://archive.apache.org/dist/couchdb/0.9.2/apache-couchdb-0.9.2.tar.gz")
 (projname . "apache-couchdb-0.9.2")
))
