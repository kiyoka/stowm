;; please download and rename jruby-1.4.0-bin,
;;   then ruby-1.4.0-jruby and tar+gzip.
(define-stowspec '(
 (conf . "")
 (make . "")
 (install . "cd .. ; /bin/rm -rf /usr/local/stow/ruby-1.4.0-jruby ; /bin/cp -r ruby-1.4.0-jruby /usr/local/stow")
 (arc . "ruby-1.4.0-jruby.tar.gz")
 (url)
 (projname . "ruby-1.4.0-jruby")
))
