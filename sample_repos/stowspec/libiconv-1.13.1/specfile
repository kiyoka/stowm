;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/libiconv-1.13.1 
")
 (make . "make")
 (install . "make install")
 (arc . "libiconv-1.13.1.tar.gz")
 (url . "http://ftp.gnu.org/pub/gnu/libiconv/libiconv-1.13.1.tar.gz")
 (projname . "libiconv-1.13.1")
))
