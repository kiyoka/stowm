;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/curl-7.20.0 
")
 (make . "make")
 (install . "make install")
 (arc . "curl-7.20.0.tar.gz")
 (url . "http://curl.haxx.se/download/curl-7.20.0.tar.gz")
 (projname . "curl-7.20.0")
))
