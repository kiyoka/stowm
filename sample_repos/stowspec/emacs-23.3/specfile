;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
command='./configure --prefix=/usr/local/stow/emacs-23.3' 
echo \" \" ============================================================
echo \" \" [[ ${command} ]]
echo \" \" ============================================================
${command}
")
 (make . "make")
 (install . "make install")
 (arc . "emacs-23.3.tar.gz")
 (url . "http://core.ring.gr.jp/pub/GNU/emacs/emacs-23.3.tar.gz")
 (projname . "emacs-23.3")
))
