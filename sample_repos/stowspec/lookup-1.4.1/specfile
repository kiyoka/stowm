;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/lookup-1.4.1 
")
 (make . "make")
 (install . "make install")
 (arc . "lookup-1.4.1.tar.gz")
 (url . "http://openlab.ring.gr.jp/edict/lookup/dist/lookup-1.4.1.tar.gz")
 (projname . "lookup-1.4.1")
))
