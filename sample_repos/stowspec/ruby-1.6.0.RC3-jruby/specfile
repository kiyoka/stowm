;;-*- mode: scheme; syntax: scheme -*-;;
;; please download and rename from jruby-1.5.3-bin to ruby-1.6.0.RC3-jruby,
;;   then tar+gzip.
(define-stowspec '(
 (conf . "")
 (make . "")
 (install . "cd ..
/bin/rm -rf /usr/local/stow/ruby-1.6.0.RC3-jruby
/bin/cp -r ruby-1.6.0.RC3-jruby /usr/local/stow
/bin/cp -f /usr/local/stow/ruby-1.6.0.RC3-jruby/bin/jruby /usr/local/stow/ruby-1.6.0.RC3-jruby/bin/ruby
")
 (arc . "ruby-1.6.0.RC3-jruby.tar.gz")
 (url)
 (projname . "ruby-1.6.0.RC3-jruby")
))
