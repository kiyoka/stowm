;;-*- mode: scheme; syntax: scheme -*-;;
;; please download and rename from jruby-1.5.2-bin to ruby-1.5.2-jruby,
;;   then ruby-1.5.2-jruby and tar+gzip.
(define-stowspec '(
 (conf . "")
 (make . "")
 (install . "cd ..
/bin/rm -rf /usr/local/stow/ruby-1.5.2-jruby
/bin/cp -r ruby-1.5.2-jruby /usr/local/stow
/bin/cp -f /usr/local/stow/ruby-1.5.2-jruby/bin/jruby /usr/local/stow/ruby-1.5.2-jruby/bin/ruby
")
 (arc . "ruby-1.5.2-jruby.tar.gz")
 (url)
 (projname . "ruby-1.5.2-jruby")
))
