;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/chicken-4.4.0 
")
 (make . "make")
 (install . "make install")
 (arc . "chicken-4.4.0.tar.gz")
 (url . "http://www.call-with-current-continuation.org/chicken-4.4.0.tar.gz")
 (projname . "chicken-4.4.0")
))
