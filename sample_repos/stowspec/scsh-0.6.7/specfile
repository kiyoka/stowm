;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/scsh-0.6.7 
")
 (make . "make")
 (install . "make install")
 (arc . "scsh-0.6.7.tar.gz")
 (url . "http://ftp.scsh.net/pub/scsh/0.6/scsh-0.6.7.tar.gz")
 (projname . "scsh-0.6.7")
))
