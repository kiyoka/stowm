;;   then tar+gzip.
(define-stowspec '(
 (conf . "")
 (make . "")
 (install . "cd ..
/bin/rm -rf /usr/local/stow/jdk-1.7.0_05
/bin/cp -r jdk-1.7.0_05 /usr/local/stow
")
 (arc . "jdk-1.7.0_05.tar.gz")
 (url)
 (projname . "jdk-1.7.0_05")
))
