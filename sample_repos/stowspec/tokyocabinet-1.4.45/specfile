;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/tokyocabinet-1.4.45 --disable-bzip 
")
 (make . "make")
 (install . "make install")
 (arc . "tokyocabinet-1.4.45.tar.gz")
 (url . "http://1978th.net/tokyocabinet/tokyocabinet-1.4.45.tar.gz")
 (projname . "tokyocabinet-1.4.45")
))
