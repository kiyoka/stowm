;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
command='./configure --prefix=/usr/local/stow/ddskk-15.1' 
echo \" \" ============================================================
echo \" \" [[ ${command} ]]
echo \" \" ============================================================
${command}
")
 (make . "make")
 (install . "make install")
 (arc . "ddskk-15.1.tar.gz")
 (url . "http://openlab.ring.gr.jp/skk/maintrunk/ddskk-15.1.tar.gz")
 (projname . "ddskk-15.1")
))
