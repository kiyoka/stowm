;;-*- mode: scheme; syntax: scheme -*-;;
(define-stowspec '(
 (conf . "if [ ! -e ./configure ] ; then
  echo 'Error: stowspec supports ./configure script included tar ball only, sorry...'
  exit 1
fi
./configure --prefix=/usr/local/stow/tokyotyrant-1.1.40 
")
 (make . "make")
 (install . "make install")
 (arc . "tokyotyrant-1.1.40.tar.gz")
 (url . "http://1978th.net/tokyotyrant/tokyotyrant-1.1.40.tar.gz")
 (projname . "tokyotyrant-1.1.40")
))
